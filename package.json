{
  "name": "mercury-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "js-base64": "^2.4.3",
    "jsonwebtoken": "^8.1.1",
    "moment": "^2.20.1",
    "pjax": "https://github.com/rishabhgrg/pjax",
    "react": "^16.2.0",
    "react-component-octicons": "^1.5.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.1.0",
    "react-syntax-highlighter": "^7.0.0",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "websocket-as-promised": "^0.6.0"
  },
  "config": {
    "pack": "build/. -p keys/development_key.pem -o dist/mercury.crx"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start-local": "REACT_APP_BACKEND_ENV=local npm run start",
    "build": "react-app-rewired build && node scripts/helpers.js",
    "gen-crx": "./node_modules/.bin/crx pack $npm_package_config_pack",
    "pack": "npm run build && npm run gen-crx",
    "test": "react-app-rewired test --env=jsdom",
    "gen-zip": "rm -f bundle.zip && zip -r9 -r9 bundle.zip build/*",
    "chrome-zip": "REACT_APP_CHROME_ZIP=true npm run build && npm run gen-zip",
    "eject": "react-app-rewired eject"
  },
  "devDependencies": {
    "crx": "^3.2.1",
    "react-app-rewired": "^1.4.1",
    "write-file-webpack-plugin": "^4.2.0"
  }
}
