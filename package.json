{
  "name": "mercury-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node update-manifest.js",
    "pack":
      "npm run build && ./node_modules/.bin/crx pack build/. -p test_key.pem -o dist/mercury.crx",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "crx": "^3.2.1"
  }
}
